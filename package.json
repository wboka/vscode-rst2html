{
  "name": "vscode-rst2html",
  "displayName": "reStructuredText 2 HTML",
  "description": "Compile reStructuredText into HTML in Visual Studio Code",
  "version": "0.0.3",
  "private": true,
  "publisher": "BokaSolutions",
  "engines": {
    "vscode": "^1.28.0"
  },
  "license": "MIT",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.convertrST2html"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.convertrST2html",
        "title": "Convert rST 2 HTML"
      }
    ]
  },
  "keywords": [
    "reStructuredText",
    "HTML",
    "convert"
  ],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test",
    "format": "prettier --write **/*.{ts,json}"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wboka/vscode-rst2html"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^7.0.43",
    "prettier": "1.17.1",
    "tslint": "^5.8.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^2.6.1",
    "vscode": "^1.1.34"
  },
  "dependencies": {
    "rst2html": "^1.0.4"
  },
  "galleryBanner": {
    "color": "#5a6f8e",
    "theme": "dark"
  },
  "icon": "rst2html.png"
}
